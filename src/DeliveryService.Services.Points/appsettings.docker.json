{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "mongo": {
    "connectionString": "mongodb://mongo:27017",
    "database": "DeliveryService-Points",
    "seed": true
  },
  "neo4j": {
    "connectionString": "bolt://neo4j:7687",
    "username": "neo4j",
    "password": "newpassword"
  },
  "rabbitmq": {
    "Username": "guest",
    "Password": "guest",
